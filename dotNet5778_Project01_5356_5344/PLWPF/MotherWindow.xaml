<Window x:Class="PLWPF.MotherWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="MotherWindow" Height="300" Width="300">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="184" Margin="-1,85,0,0" VerticalAlignment="Top" Width="292">
            <Button x:Name="newMother" Content="New Mother" Height="46" Margin="1,0,0,0" Width="291" Click="newMother_Click" />
            <Button x:Name="existingMother" Content="Mother Entry"  Height="46" Margin="1,0,0,0" Width="291" Click="MotherUserEnter"/>
            <Button x:Name="button_Copy1" Content="hackonamatta"  Height="46" Margin="1,0,0,0" Width="291" />
            <Button x:Name="button_Copy2" Content="back"  Height="46" Margin="1,0,0,0" Width="291"/>
        </StackPanel>


    </Grid>
</Window>
